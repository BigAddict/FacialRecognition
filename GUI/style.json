{
	"QMainWindow":[
		{
			"tittle":"Facial Recognition",
			"icon":":/icons/Icons/icon.png",
			"frameless": true,
			"shadow":[
				{
					"color": "#fff",
					"blurRadius": 20,
					"xOffset": 0,
					"yOffset": 0,
					"centralWidget": "mainContainer"
				}
			],
			"navigation":[
				{
					"minimize":"minimizeBtn",
					"close": "closeBtn",
					"restore":[
						{	
							"buttonName": "restoreBtn",
							"normalIcon": ":/icons/Icons/square.png",
							"maximizedIcon": ":/icons/Icons/copy.png"
						}
					],
					"moveWindow": "headerContainer",
					"tittleBar": "headerContainer"
				}
			]
		}
	],
    "QStackedWidget":[
        {
            "name":"stackedWidget",
            "transitionAnimation":[
                {
                    "fade":[
                        {
                            "active":true,
                            "duration":1000,
                            "easingCurve":"OutBack"
                        }
                    ],
                    "slide":[
                        {
                            "active":true,
                            "duration":1000,
                            "direction":"horizontal",
                            "easingCurve":"OutBack"
                        }
                    ]
                }
            ],
            "navigation":[
                {
                    "navigationButtons":[
                        {
                            "datasetBtn":"imageDataset",
                            "recognitionBtn":"recognitionSettings"
                        }
                    ]
                }
            ]
        }
    ]
}